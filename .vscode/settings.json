{
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/node_modules": false
  },
  "javascript.suggest.jsdoc.generateReturns": false,
  "typescript.suggest.jsdoc.generateReturns": false
}